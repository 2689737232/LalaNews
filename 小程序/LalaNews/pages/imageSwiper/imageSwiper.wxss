/* pages/imageSwiper/imageSwiper.wxss */
.container,page{
    width: 100%;
    height: 100%;
    background-color: black;
}
.container{
    padding: 0;
    position: relative;
}
.base-view{
    width: 100%;
    height: 100%;
}
.swiper-image{
    position: absolute;
    top: 20%;
    width: 100%;
    height: 50%;
    background-color: brown;
    z-index: 99;
}
.image-item{
    width: 100%;
    height: 100%;
}